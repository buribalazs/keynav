{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
//    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["keynav.js"],
      "css": ["keynav.css"]
    }
  ]
//  "permissions": [
//    "activeTab",
//    "https://ajax.googleapis.com/"
//  ]
}
